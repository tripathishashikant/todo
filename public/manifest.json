{
	"manifest_version": 1,
	"short_name": "Todo",
	"name": "Todo App",
	"version": "v1.0",
	"content_security_policy": "default-src 'self' data:; script-src 'self' 'unsafe-inline' code.jquery.com; connect-src 'self'; img-src 'self' data:; style-src 'self' fonts.googleapis.com; font-src 'self' data: fonts.gstatic.com",
	"background_color": "#1f2024",
	"theme_color": "#ffffff",
	"icons": [
		{
			"src": "src/assets/images/favicons/favicon-16x16.png",
			"sizes": "16x16",
			"type": "image/png",
			"density": "4.0"
		},
		{
			"src": "src/assets/images/favicons/favicon-32x32.png",
			"sizes": "32x32",
			"type": "image/png",
			"density": "4.0"
		},
		{
			"src": "src/assets/images/favicons/favicon-96x96.png",
			"sizes": "96x96",
			"type": "image/png",
			"density": "4.0"
		},
		{
			"src": "src/assets/images/favicons/favicon-180x180.png",
			"sizes": "180x180",
			"type": "image/png",
			"density": "4.0"
		},
		{
			"src": "src/assets/images/favicons/favicon-192x192.png",
			"sizes": "192x192",
			"type": "image/png",
			"density": "4.0"
		},
		{
			"src": "src/assets/images/favicons/favicon-512x512.png",
			"sizes": "512x512",
			"type": "image/png",
			"density": "4.0"
		}
	],
	"start_url": "/index.html",
	"scope": ".",
	"orientation": "portrait-primary",
	"display": "standalone",
	"dir": "ltr",
	"lang": "en"
}